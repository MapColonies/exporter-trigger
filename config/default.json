{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "jobManager": {
    "url": "http://job-manager-job-manager",
    "expirationTime": 30
  },
  "rasterCatalogManager": {
    "url": "http://raster-catalog-manager"
  },
  "workerTypes": {
    "tiles": {
      "jobType": "rasterTilesExporter",
      "taskType": "rasterTilesExporter"
    }
  },
  "tilesProvider": "S3"
}
